# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=váš_firebase_api_klíč
REACT_APP_FIREBASE_AUTH_DOMAIN=váš_projekt.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=váš_projekt_id
REACT_APP_FIREBASE_STORAGE_BUCKET=váš_projekt.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=vaše_sender_id
REACT_APP_FIREBASE_APP_ID=vaše_app_id

# MQTT Configuration - WebSocket (pro HTTP)
REACT_APP_MQTT_URL=ws://89.24.76.191:9001/mqtt

# MQTT Configuration - Secure WebSocket (pro HTTPS)  
REACT_APP_MQTT_WSS_URL=wss://89.24.76.191:9002/mqtt

# Camera Configuration
REACT_APP_CAMERA_URL=http://89.24.76.191:8080/video_feed

# MQTT Polling Configuration (pro HTTP fallback)
REACT_APP_MQTT_POLL_INTERVAL_MS=7000
REACT_APP_MQTT_POLL_HIDDEN_INTERVAL_MS=20000

# Performance Optimization
REACT_APP_CONNECTION_TIMEOUT_MS=10000
REACT_APP_RETRY_ATTEMPTS=5
REACT_APP_HEALTH_CHECK_INTERVAL_MS=30000

# Debug Mode
REACT_APP_DEBUG_MQTT=true
REACT_APP_DEBUG_CONNECTIONS=true
